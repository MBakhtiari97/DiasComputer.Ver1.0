@model List<DiasComputer.DataLayer.Entities.SiteResources.Banner>
@foreach (var banner in Model)
{
    <div class="col-6 col-lg-3">
        <div class="widget-banner card">
            <a href="@banner.BannerRedirectTo" target="_blank">
                <img class="img-fluid" src="/images/banner/@banner.BannerImg" alt="@banner.BannerAltName">
            </a>
        </div>
    </div>
}
